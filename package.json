    "start-dev": "ts-node ./src/services/index.ts",
    "start-prod": "cross-env NODE_ENV=production ts-node ./src/services/index.ts",
    "test": "mocha --timeout 20000 --require ts-node/register --require source-map-support/register --recursive \"./test/**/*.ts\"",
    "test-cover": "nyc npm run test",
    "test-server": "mocha --timeout 20000 --require ts-node/register --require source-map-support/register --recursive \"./test/services.spec.ts\"",
    "tsc-build": "tsc --listEmittedFiles",
    "tsc-check": "tsc --noEmit --listEmittedFiles"
