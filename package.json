    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "mocha": "^4.1.0",
    "nyc": "^11.3.0",
    "prettier": "^2.0.5",
