  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/chai-arrays": "^1.0.3",
    "@types/colors": "^1.2.1",
    "@types/cors": "^2.8.6",
    "@types/css-selector-tokenizer": "^0.7.0",
    "@types/express": "^4.17.6",
