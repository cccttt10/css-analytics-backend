    "dist": "sh ./dist.sh",
    "eslint-check": "eslint --no-error-on-unmatched-pattern src/**/*.ts src/**/*.js test/**/*.ts",
    "eslint-fix": "eslint --no-error-on-unmatched-pattern src/**/*.ts src/**/*.js test/**/*.ts --fix",
    "fix": "npm run prettier-fix && npm run eslint-fix",
    "prettier-check": "prettier --config ./.prettierrc.json --check \"./src/**/*.ts\" \"./src/**/*.js\" \"./test/**/*.ts\"",
